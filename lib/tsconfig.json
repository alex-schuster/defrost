    "noEmitOnError": true,
    "removeComments": false,
    "downlevelIteration": true,
    "sourceMap": true,
    "target": "es2015",
    "outDir": "dist",
    "module": "commonjs"
  },
  "include": [
    "src"
  ],
  "exclude": [
    "dist",
    "node_modules"
  ]
}
